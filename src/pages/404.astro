---
import BaseLayout from '../layouts/BaseLayout.astro';
import ContentWithHeading from '../components/ContentWithHeading.astro';

Astro.response.status = 404;
---
<BaseLayout>
    <ContentWithHeading contentTitle="404">
        <h1>page not found<span class="text-emphasis-blue">.</span></h1>
        <p>
            The page you're looking for doesn't exist. Return to <a href="/">/home</a>.
        </p>
    </ContentWithHeading>
</BaseLayout>

